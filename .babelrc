{
  "sourceMaps": true,
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["Safari >= 5.1"]
        },
        "useBuiltIns": false,
        "loose": true,
        "modules": false,
        "debug": false
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-react-jsx", { "pragma": "h" }]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["Safari >= 5.1"]
            },
            "modules": "commonjs"
          }
        ],
        "stage-0"
      ]
    },
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "remove",
            "removeImport": true
          }
        ]
      ]
    }
  }
}
