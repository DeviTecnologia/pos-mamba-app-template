{
  "name": "preact-boilerplate",
  "version": "6.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "author": "Stone Payments - Mamba Team",
  "license": "Apache-2.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.build.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config tools/webpack/webpack.config.build.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.analyzer.js",
    "test": "jest --coverage",
    "test:watch": "npm run -s test -- --watch",
    "lint": "npm run lint:css; npm run lint:js",
    "lint:js": "eslint *.js '{src,test,webpack}/**/*.{js,jsx}'",
    "lint:css": "stylelint 'src/**/*.{css,sass,scss,sss,less}'",
    "format": "npm run format:css; npm run format:js",
    "format:js": "prettier --write *.js '{src,test,webpack}/**/*.{js,jsx}' && eslint --fix *.js '{src,test,webpack}/**/*.{js,jsx}'",
    "format:css": "prettier --write 'src/**/*.{css,sass,scss,sss,less}' && stylelint --fix 'src/**/*.{css,sass,scss,sss,less}'"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "autoprefixer": "^8.2.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-minifier": "^3.5.14",
    "husky": "^0.15.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "inspect-loader": "^1.0.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "mini-html-webpack-plugin": "^0.2.3",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-scss": "^1.0.5",
    "preact-render-spy": "^1.2.2",
    "prettier": "^1.11.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "history": "^4.7.2",
    "phantomjs-function-bind-polyfill": "^1.0.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "preact-router": "^2.6.0"
  }
}
